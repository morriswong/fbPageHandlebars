        <span class="list-group-item-text">Likes: {{fan_count}}</span>
        <span class="list-group-item-text">About:{{about}}</span>
    </a>
        <div>
            <a href="#{{id}}"><img id="{{id}}" class="unlike" src="../../assets/svg/{{favor}}.svg" alt="heart"></a>
        </div>
    {{/selectedCategory}}
</div>
</div>

